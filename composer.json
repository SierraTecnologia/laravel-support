{
    "name": "sierratecnologia/laravel-support",
    "description": "SierraTecnologia common support helpers, contracts, and traits required by various SierraTecnologia packages. Validator functionality, and basic controller included out-of-the-box.",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "laravel",
        "contract",
        "validator",
        "timezones",
        "mimetype",
        "support",
        "helper",
        "trait"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/marketplace/sierratecnologia-support/",
    "support": {
        "email": "help@sierratecnologia.com",
        "issues": "https://github.com/sierratecnologia/laravel-support/issues",
        "source": "https://github.com/sierratecnologia/laravel-support",
        "docs": "https://github.com/sierratecnologia/laravel-support/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia LLC",
            "homepage": "https://sierratecnologia.com",
            "email": "help@sierratecnologia.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/sierratecnologia/laravel-support/contributors"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/auth": "^5.8|^6.0",
        "illuminate/bus": "^5.8|^6.0",
        "illuminate/contracts": "^5.8|^6.0",
        "illuminate/http": "^5.8|^6.0",
        "illuminate/routing": "^5.8|^6.0",
        "illuminate/support": "^5.8|^6.0",
        "illuminate/validation": "^5.8|^6.0",
        "laravel/helpers": "^1.1.0",
        "spatie/laravel-schemaless-attributes": "^1.6.0",
        "spatie/laravel-translatable": "^4.2",
        "ricardosierra/translation": "dev-master",
        "watson/validating": "^3.3.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^8.3.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
