{
    "name": "sierratecnologia/laravel-support",
    "description": "SierraTecnologia common support helpers, contracts, and traits required by various SierraTecnologia packages. Validator functionality, and basic controller included out-of-the-box.",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "laravel",
        "contract",
        "validator",
        "timezones",
        "mimetype",
        "support",
        "helper",
        "trait"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/marketplace/sierratecnologia-support/",
    "support": {
        "email": "help@sierratecnologia.com",
        "issues": "https://github.com/sierratecnologia/laravel-support/issues",
        "source": "https://github.com/sierratecnologia/laravel-support",
        "docs": "https://github.com/sierratecnologia/laravel-support/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia LLC",
            "homepage": "https://sierratecnologia.com",
            "email": "help@sierratecnologia.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "cviebrock/eloquent-sluggable": ">=4.0",
        "doctrine/orm": ">=2.5",
        "illuminate/auth": "^6.0|^7.0|^8.0",
        "illuminate/bus": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "laravel/framework": "^6.0|^7.0",
        "laravelcollective/html": "^5.0|^6.0|^7.0|^8.0",
        "doctrine/dbal": ">=2.5",
        "intervention/image": ">=2.4",
        "larapack/doctrine-support": ">=0.1",
        "laracasts/presenter": ">=0.2",
        "bkwld/croppa": "^4.0",
        "ricardosierra/laravel-haml": "^2.0",
        "league/flysystem": ">=1.0.41",
        "ricardosierra/library": "^4.0",
        "bkwld/cloner": "^3.0",
        "ricardosierra/entrust": "dev-master",
        "symfony/yaml": ">=2.5",
        "jenssegers/agent": ">=2.1",
        "zencoder/zencoder-php": ">=2.2",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/validation": "^6.0|^7.0|^8.0",
        "larapack/hooks": ">=1.0.5",
        "laravel/helpers": ">=1.1.0",
        "laravel/tinker": "^2.0",
        "league/fractal": ">=0.18.0",
        "nicolaslopezj/searchable": ">=1.10",
        "ramsey/uuid": ">=3.8",
        "anahkiasen/html-object": ">=1.4",
        "zendframework/zend-crypt": "*",
        "laracasts/flash": "^3.0",
        "sierratecnologia/former": "^5.0.4",
        "sierratecnologia/crudmaker": "dev-master",
        "sierratecnologia/crypto": "dev-master",
        "sierratecnologia/muleta": "dev-master",
        "ricardosierra/changelog": "^1.4",
        "ricardosierra/minify": "dev-master",
        "elasticquent/elasticquent": "*",
        "predis/predis": ">=1.1",
        "sierratecnologia/locaravel": "dev-master",
        "ricardosierra/validate": "^0.2",
        "graham-campbell/markdown": "*",
        "rap2hpoutre/fast-excel": "^1.0",
        "league/pipeline": "*",
        "league/climate": ">=3.4",
        "google/recaptcha": "*",
        "spatie/laravel-backup": "^6.0|^5.0",
        "maatwebsite/excel": ">=3.1",
        "kartik-v/bootstrap-fileinput": "dev-master",
        "pusher/pusher-php-server": "^3.0|^4.0",
        "jlapp/swaggervel": "dev-master",
        "prettus/l5-repository": "*",
        "hieu-le/active": "^7.0",
        "sabre/xml": "*",
        "league/csv": "^9.0|^8.0|^7.1",
        "spatie/eloquent-sortable": ">=3.7",
        "spatie/laravel-schemaless-attributes": ">=1.6.0",
        "spatie/laravel-sluggable": ">=2.2",
        "symfony/inflector": ">=5.0",
        "venturecraft/revisionable": ">=1.30",
        "felixkiss/uniquewith-validator": "^3.4.0",
        "watson/validating": "^5.0.0",
        "ricardosierra/phpwhois": "dev-dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": ">=2.7",
        "codedungeon/phpunit-result-printer": ">=0.27.0",
        "phpunit/phpunit": ">=8.3.0"
    },
    "autoload": {
        "psr-4": {
            "Support\\": "src/"
        },
        "files": [
            "utils/funcs/load-laravel.php",
            "utils/funcs/helpers.php",
            "utils/funcs/form_maker.php",
            "utils/funcs/helperSlugify.php",
            "utils/funcs/input_maker.php",
            "utils/funcs/Reflection.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Support\\SupportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
